<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="pemesanan_ruangan_action_id" model="ir.actions.act_window">
        <field name="name">Pemesanan Ruangan</field>
        <field name="res_model">pemesanan.ruangan</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Klik untuk membuat pemesanan ruangan baru.
            </p>
        </field>
    </record>

    <record id="pemesanan_actions_tree_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="1"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="pemesanan_ruangan.pemesanan_ruangan_tree_view_id"/>
        <field name="act_window_id" ref="pemesanan_ruangan.pemesanan_ruangan_action_id"/>
    </record>
</odoo>
